<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.nftf.kys.mapper.BoardMapper">
	<select id="getBoard" resultType="Board">
		SELECT *
		FROM board
		WHERE id =
		#{id}
	</select>
</mapper>